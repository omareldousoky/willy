  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "start-login": "webpack-dev-server --mode development --env.development --config ./config/webpack.login.js --hot --port 8080",
    "start-mohassel": "webpack-dev-server --mode development --env.development --config ./config/webpack.mohassel.js --hot --port 8081",
    "start-documents": "webpack-dev-server --mode development --env.development --config ./config/webpack.documents.js --hot --port 8081",
    "start-cf": "webpack-dev-server --mode development --env.development --config ./config/webpack.cf.js --hot --port 8081",
    "build": "npm run build-mohassel & npm run build-cf & npm run build-login & npm run build-documents",
    "build-login": "webpack --mode production --env.production --config ./config/webpack.login.js",
    "build-mohassel": "webpack --mode production --env.production --config ./config/webpack.mohassel.js",
    "type-check-login": "webpack --mode development --env.development --config ./config/webpack.login.js",
    "type-check-mohassel": "webpack --mode development --env.development --config ./config/webpack.mohassel.js",
    "type-check-documents": "webpack --mode development --env.development --config ./config/webpack.documents.js",
    "type-check-cf": "webpack --mode development --env.development --config ./config/webpack.cf.js",
    "type-check": "npm run type-check-mohassel & npm run type-check-cf & npm run type-check-login & npm run type-check-documents",
    "build-documents": "webpack --mode production --env.production --config ./config/webpack.documents.js",
    "build-cf": "webpack --mode production --env.production --config ./config/webpack.cf.js",
    "lint-login": "eslint --cache src/Login --ext .ts,.tsx --no-error-on-unmatched-pattern",
    "lint-login:fix": "eslint --cache src/Login --ext .ts,.tsx --no-error-on-unmatched-pattern --fix",
    "lint-mohassel": "eslint --cache src/Mohassel --ext .ts,.tsx --no-error-on-unmatched-pattern",
    "lint-mohassel:fix": "eslint --cache src/Mohassel --ext .ts,.tsx --no-error-on-unmatched-pattern --fix",
    "lint-documents": "eslint --cache src/Documents --ext .ts,.tsx --no-error-on-unmatched-pattern",
    "lint-documents:fix": "eslint --cache src/Documents --ext .ts,.tsx --no-error-on-unmatched-pattern --fix",
    "lint-shared": "eslint --cache src/Shared --ext .ts,.tsx --no-error-on-unmatched-pattern",
    "lint-shared:fix": "eslint --cache src/Shared --ext .ts,.tsx --no-error-on-unmatched-pattern --fix",
    "lint-cf": "eslint --cache src/CF --ext .ts,.tsx --no-error-on-unmatched-pattern",
    "lint-cf:fix": "eslint --cache src/CF --ext .ts,.tsx --no-error-on-unmatched-pattern --fix",
    "cf-dev": "sh ./dev-env.sh cf dev",
    "cf-test": "sh ./dev-env.sh cf test",
    "lts-dev": "sh ./dev-env.sh lts dev",
    "lts-test": "sh ./dev-env.sh lts test",
    "lts-docs-dev": "sh ./dev-env.sh lts-docs dev",
    "lts-docs-test": "sh ./dev-env.sh lts-docs test"
  },
