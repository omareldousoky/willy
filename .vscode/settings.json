{
  "eslint.debug": true,
  "eslint.format.enable": true,
  "eslint.validate": [
    "html",
    "javascript",
    "typescript",
    "javascriptreact",
    "typescriptreact"
  ],
  "eslint.options": {
    "extensions": [".tsx", ".ts", ".js"]
  },
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": true
  }
}
